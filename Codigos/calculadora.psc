Proceso calculadora
	definir num1, num2 Como Entero;
	Definir var1 Como Caracter;
	Definir Res Como Entero;
	Escribir "S,M,D,R";
	Si var1="S" Entonces
		Escribir "Suma";
	FinSi
	Si var1="M" Entonces
		Escribir "Multiplicacion";
	FinSi
	Si var1="D" Entonces
		Escribir "Division";
	FinSi
	Si var1="R" Entonces
		Escribir "Resta";
	FinSi
	Escribir"Ingresa un numero";
	Leer num1;
	Escribir "Ingresa un numero";
	Leer num2;
	Escribir"El resultado es" ,Res;
	
FinProceso
